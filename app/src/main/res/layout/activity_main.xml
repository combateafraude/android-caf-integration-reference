<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="io.caf.sdk.integration.reference.MainActivity">

    <Button
        android:id="@+id/btCafFaceliveness"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/caf_faceliveness"
        app:layout_constraintBottom_toTopOf="@id/btCafDocumentDetector"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btCafDocumentDetector"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/caf_documentdetector"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btCafFaceliveness" />
</androidx.constraintlayout.widget.ConstraintLayout>